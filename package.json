{
	"name": "coral",
	"version": "0.1.0",
	"private": true,
	"license": "private",
	"scripts": {
		"dev": "next dev",
		"build": "ANALYZE=false && next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"storybook": "start-storybook -p 6006",
		"type-check": "tsc --pretty --noEmit",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"verify": "run-p prettier lint",
		"verify:fix": "run-p prettier:fix lint:fix",
		"test": "jest",
		"test-all": "yarn lint && yarn type-check && yarn test",
		"test:ci": "jest --ci",
		"generate": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config codegen.yml",
		"generate:watch": "yarn generate --watch",
		"prestart": "yarn generate && yarn lint",
		"predev": "yarn generate && yarn lint",
		"ngrok-server": "ngrok http 1337",
		"ngrok-ui": "ngrok http 3000",
		"analyze": "cross-env ANALYZE=true next build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn run type-check"
		}
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn prettier"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.6.6",
		"@chakra-ui/react": "^2.1.2",
		"@chakra-ui/theme-tools": "^2.0.1",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11",
		"@fontsource/hind": "^4.5.0",
		"@fontsource/inter": "^4.5.11",
		"@fontsource/montserrat": "^4.5.1",
		"@hookform/error-message": "^2.0.0",
		"@nhost/nextjs": "^1.2.10",
		"@nhost/react": "^0.7.10",
		"@nhost/react-apollo": "^4.2.13",
		"@nhost/react-auth": "^2.0.3",
		"@paypal/react-paypal-js": "^7.8.0",
		"@reduxjs/toolkit": "^1.8.2",
		"@types/node": "^17.0.40",
		"chakra-ui-steps": "^1.3.0",
		"date-fns": "^2.28.0",
		"focus-visible": "^5.2.0",
		"framer-motion": "^5.6.0",
		"graphql": "^16.5.0",
		"graphql-tag": "^2.12.6",
		"next": "latest",
		"react": "^18.1.0",
		"react-cropper": "^2.1.8",
		"react-datepicker": "^4.6.0",
		"react-device-detect": "^2.2.2",
		"react-dom": "^18.1.0",
		"react-dropzone": "^11.5.1",
		"react-google-login": "^5.2.2",
		"react-hook-form": "^7.27.1",
		"react-icons": "^4.4.0",
		"react-redux": "^8.0.2",
		"react-select": "^5.2.2",
		"react-textarea-autosize": "^8.3.3",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.18.5",
		"@babel/preset-env": "^7.18.2",
		"@babel/preset-typescript": "^7.17.12",
		"@graphql-codegen/cli": "^2.3.1",
		"@graphql-codegen/introspection": "^2.1.0",
		"@graphql-codegen/typescript": "^2.4.2",
		"@graphql-codegen/typescript-operations": "^2.2.2",
		"@graphql-codegen/typescript-react-apollo": "^3.2.4",
		"@graphql-codegen/typescript-resolvers": "^2.4.0",
		"@next/bundle-analyzer": "^12.1.5",
		"@testing-library/dom": "^8.11.2",
		"@testing-library/jest-dom": "^5.16.4",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^28.1.1",
		"@types/react": "^17.0.38",
		"@types/react-datepicker": "^4.1.7",
		"@types/react-dom": "^17.0.11",
		"@types/react-dropzone": "^5.1.0",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"@xstate/inspect": "^0.7.0",
		"babel-jest": "^28.1.1",
		"cross-env": "^7.0.3",
		"dotenv": "^16.0.1",
		"eslint": "^8.16.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-next": "^12.1.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"hasura-cli": "^2.7.0",
		"husky": "^7.0.4",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^28.1.1",
		"jest-environment-jsdom": "^28.1.1",
		"jest-watch-typeahead": "^1.1.0",
		"lint-staged": "^13.0.0",
		"prettier": "^2.6.2",
		"ts-jest": "^28.0.4",
		"ts-node": "^10.8.1",
		"typescript": "4.6.4",
		"ws": "^8.7.0"
	}
}
