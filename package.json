{
	"name": "coral",
	"version": "0.1.0",
	"private": true,
	"license": "private",
	"scripts": {
		"dev": "next dev",
		"build": "ANALYZE=false && next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start",
		"storybook": "start-storybook -p 6006",
		"type-check": "tsc --pretty --noEmit",
		"prettier": "prettier --check .",
		"prettier:fix": "prettier --write .",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"verify": "run-p prettier lint",
		"verify:fix": "run-p prettier:fix lint:fix",
		"test": "jest",
		"test-all": "yarn lint && yarn type-check && yarn test",
		"test:ci": "jest --ci",
		"generate": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config codegen.yml",
		"generate:watch": "yarn generate --watch",
		"prestart": "yarn generate && yarn lint",
		"predev": "yarn generate && yarn lint",
		"ngrok-server": "ngrok http 1337",
		"ngrok-ui": "ngrok http 3000",
		"analyze": "cross-env ANALYZE=true next build",
		"analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
		"analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn run type-check"
		}
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn prettier"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.7.13",
		"@chakra-ui/react": "^2.6.0",
		"@chakra-ui/theme-tools": "^2.0.16",
		"@emotion/react": "^11.10.8",
		"@emotion/styled": "^11.10.8",
		"@fontsource/hind": "^4.5.10",
		"@fontsource/inter": "^4.5.15",
		"@fontsource/montserrat": "^4.5.14",
		"@hookform/error-message": "^2.0.1",
		"@nhost/nextjs": "^1.13.21",
		"@nhost/react": "^2.0.15",
		"@nhost/react-apollo": "^5.0.19",
		"@nhost/react-auth": "^3.6.0",
		"@paypal/payouts-sdk": "^1.1.1",
		"@paypal/react-paypal-js": "^7.8.3",
		"@reduxjs/toolkit": "^1.9.5",
		"chakra-ui-steps": "^2.1.0",
		"date-fns": "^2.28.0",
		"focus-visible": "^5.2.0",
		"framer-motion": "^10.12.4",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"next": "^13.3.1",
		"node-fetch": "^2.6.1",
		"react": "^18.2.0",
		"react-cropper": "^2.3.3",
		"react-datepicker": "^4.11.0",
		"react-device-detect": "^2.2.3",
		"react-dom": "^18.2.0",
		"react-dropzone": "^14.2.3",
		"react-google-login": "^5.2.2",
		"react-hook-form": "^7.43.9",
		"react-icons": "^4.8.0",
		"react-redux": "^8.0.5",
		"react-select": "^5.7.2",
		"react-textarea-autosize": "^8.4.1",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.21.5",
