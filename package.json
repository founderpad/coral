{
	"name": "coral",
	"version": "0.1.0",
	"private": true,
	"license": "private",
	"scripts": {
		"dev": "next dev",
		"build": "yarn test && next build",
		"start": "next start",
		"storybook": "start-storybook -p 6006",
		"type-check": "tsc --pretty --noEmit",
		"prettier": "prettier --write .",
		"lint": "next lint",
		"test": "jest",
		"test-all": "yarn lint && yarn type-check && yarn test",
		"test:ci": "jest --watch",
		"generate": "DOTENV_CONFIG_PATH=./.env.local graphql-codegen -r dotenv/config --config codegen.yml",
		"generate:watch": "yarn generate --watch",
		"prestart": "yarn generate",
		"predev": "yarn generate",
		"ngrok": "ngrok http 3000"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn run type-check"
		}
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn prettier"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.5.7",
		"@chakra-ui/react": "^1.6.8",
		"@chakra-ui/theme-tools": "^1.2.1",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11",
		"@fontsource/hind": "^4.5.0",
		"@fontsource/inter": "^4.5.1",
		"@fontsource/montserrat": "^4.5.1",
		"@nhost/nhost-js": "^0.3.5",
		"@nhost/react-apollo": "^2.1.0",
		"@nhost/react-auth": "^2.0.3",
		"@reduxjs/toolkit": "^1.6.1",
		"chakra-ui-steps": "^1.3.0",
		"date-fns": "^2.28.0",
		"focus-visible": "^5.2.0",
		"framer-motion": "^5.6.0",
		"graphql": "^16.2.0",
		"graphql-tag": "^2.12.6",
		"next": "^12.0.8",
		"react": "^17.0.2",
		"react-cropper": "^2.1.8",
		"react-datepicker": "^4.6.0",
		"react-dom": "^17.0.2",
		"react-dropzone": "^11.5.1",
		"react-hook-form": "7.20.2",
		"react-icons": "^4.2.0",
		"react-redux": "^7.2.6",
		"react-textarea-autosize": "^8.3.3",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.3.1",
		"@graphql-codegen/introspection": "^2.1.0",
		"@graphql-codegen/typescript": "^2.4.2",
		"@graphql-codegen/typescript-operations": "^2.2.2",
		"@graphql-codegen/typescript-react-apollo": "^3.2.4",
		"@graphql-codegen/typescript-resolvers": "^2.4.0",
		"@testing-library/dom": "^8.11.2",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.1",
		"@testing-library/user-event": "^13.5.0",
		"@types/jest": "^27.4.0",
		"@types/react": "^17.0.38",
		"@types/react-datepicker": "^4.1.7",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"babel-jest": "^27.4.6",
		"dotenv": "^14.1.0",
		"eslint": "^8.6.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"express": "^4.17.2",
		"hasura-cli": "^2.1.1",
		"husky": "^7.0.4",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.4.7",
		"jest-watch-typeahead": "^1.0.0",
		"lint-staged": "^12.1.7",
		"prettier": "^2.5.1",
		"typescript": "^4.5.4"
	}
}
