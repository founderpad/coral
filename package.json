{
	"name": "boilerplate",
	"version": "0.1.0",
	"private": true,
	"license": "private",
	"scripts": {
		"dev": "next dev",
		"build": "yarn test && next build",
		"start": "next start",
		"storybook": "start-storybook -p 6006",
		"type-check": "tsc --pretty --noEmit",
		"prettier": "prettier --write .",
		"lint": "eslint . --ext ts --ext tsx --ext js",
		"test": "jest",
		"test-all": "yarn lint && yarn type-check && yarn test",
		"generate": "graphql-codegen --config codegen.js -r",
		"generate:watch": "yarn generate --watch"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "yarn run type-check"
		}
	},
	"lint-staged": {
		"*.@(ts|tsx)": [
			"yarn lint",
			"yarn prettier"
		]
	},
	"dependencies": {
		"@apollo/client": "^3.4.10",
		"@chakra-ui/react": "^1.6.3",
		"@chakra-ui/theme-tools": "^1.1.9",
		"@emotion/react": "^11.4.1",
		"@emotion/styled": "^11",
		"@fontsource/barlow": "^4.5.0",
		"@nhost/react-apollo": "^1.1.3",
		"@nhost/react-auth": "^1.0.5",
		"@reduxjs/toolkit": "^1.6.1",
		"chakra-paginator": "^0.7.3",
		"date-fns": "^2.23.0",
		"framer-motion": "^4",
		"graphql": "^15.5.2",
		"graphql-tag": "^2.12.5",
		"next": "^11.1.2",
		"nhost-js-sdk": "^3.1.0",
		"react": "17.0.2",
		"react-cropper": "^2.1.8",
		"react-datepicker": "^4.2.1",
		"react-dom": "17.0.2",
		"react-dropzone": "^11.3.4",
		"react-hook-form": "^7.14.2",
		"react-icons": "^4.2.0",
		"react-redux": "^7.2.4",
		"redux-persist": "^6.0.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^2.1.1",
		"@graphql-codegen/introspection": "^2.1.0",
		"@graphql-codegen/typescript": "^2.1.2",
		"@graphql-codegen/typescript-operations": "^2.1.2",
		"@graphql-codegen/typescript-react-apollo": "^3.1.2",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.1.0",
		"@types/jest": "^27.0.1",
		"@types/react": "^17.0.24",
		"@types/react-datepicker": "^4.1.4",
		"@types/react-dom": "^17.0.9",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"babel-jest": "^27.2.2",
		"eslint": "^7.32.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.1",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"husky": "^7.0.2",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.2.2",
		"jest-watch-typeahead": "^0.6.4",
		"lint-staged": "^11.1.2",
		"prettier": "^2.3.2",
		"typescript": "^4.4.2"
	}
}
